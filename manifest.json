{
  "name": "Paper: New tab notebook",
  "version": "1.0",
  "description": "Clean, simple interface to write notes",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "note.html"
  },
  "background":{
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "manifest_version": 2
}
